extends layout

block content
  h1= title
  - var result = results
  div.container-fluid
     div.row
        each item,index in result
            div.col-sm-3(onclick=`buy(${index})`) Cereal Brand: #{item["cerealBrandName"]}  <br>  Cereal Flavor: #{item["cerealFlavor"]} <br> Price: #{item["price"]} 
  script.
      function buy(i){
         alert("you purchased item "+(i+1))
      }